<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="tei_odds.rnc" type="application/relax-ng-compact-syntax"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>SSK spec for form creation with the TEIMETA tool</title>
				<author>
					<name>Charles Riondet</name>
				</author>
			</titleStmt>
			<publicationStmt>
				<p/>
			</publicationStmt>
			<notesStmt>
				<note type="ns">http://www.example.org/ns/nonTEI</note>
			</notesStmt>
			<sourceDesc>
				<p>adapted from
					https://github.com/christopheparisse/teimeta/blob/master/models/ODD_Meta_niveau0_V17.odd</p>
			</sourceDesc>
		</fileDesc>
	</teiHeader>
	<text>
		<body>
			<schemaSpec ident="TEI_SSK" start="TEI" ns="http://www.tei-c.org/ns/1.0">
				<!-- ajouter teiCorpus par la suite si collections -->


				<moduleRef key="core"
					include="abbr addrLine address analytic author bibl biblScope biblStruct date desc editor email expan graphic head imprint item label lb list media meeting monogr name p postBox postCode pubPlace publisher ref resp respStmt series street term"/>
				<moduleRef key="tei"/>
				<moduleRef key="header"/>
				<moduleRef key="textstructure"/>
				<moduleRef key="namesdates"/>
				<moduleRef key="linking"/>
				<moduleRef key="tagdocs"/>
				<moduleRef key="figures"/>

				<!-- structure generale TEI -->
				<elementSpec ident="TEI" module="textstructure">
					<content>
						<sequence minOccurs="1" maxOccurs="1">
							<elementRef key="teiHeader"/>
							<elementRef key="text"/>
						</sequence>
					</content>
					<attList>
						<attDef ident="type" usage="req" mode="change">
							<datatype>
								<dataRef key="teidata.enumerated"/>
							</datatype>
							<valList type="closed" mode="replace">
								<valItem ident="researchStep"/>
								<valItem ident="researchScenario"/>
							</valList>
						</attDef>
						<attDef ident="xml:id" usage="req">
							<datatype>
								<dataRef key="anyURI"/>
							</datatype>
						</attDef>
					</attList>
				</elementSpec>

				<!-- structure generale teiHeader -->
				<elementSpec ident="teiHeader" module="header" mode="change">
					<content>
						<sequence minOccurs="1" maxOccurs="1">
							<elementRef key="fileDesc"/>
							<elementRef key="encodingDesc"/>
							<elementRef key="revisionDesc"/>
						</sequence>
					</content>
				</elementSpec>
				
				<elementSpec ident="revisionDesc"></elementSpec>

				<elementSpec ident="encodingDesc"></elementSpec>
				<elementSpec ident="text" module="textstructure">
					<!--
             unused element dont need to be defined
             they will be ignored
          -->
					<content>
						<sequence>
								<elementRef key="body"/>
						</sequence>
					</content>
				</elementSpec>

				<elementSpec ident="body" module="textstructure">
					<content>
						<elementRef key="div"/>
					</content>
				</elementSpec>

				<elementSpec ident="div" module="textstructure">
					<content>
						<sequence minOccurs="0" maxOccurs="unbounded">
							<elementRef key="head"/>
							<elementRef key="desc" corresp="definition"/>
							<elementRef key="desc" corresp="terms"/>
						</sequence>
						<sequence minOccurs="1" maxOccurs="1">
							<elementRef key="figure"/>
							<elementRef key="listEvent"/>
						</sequence>
					</content>
				</elementSpec>

				<elementSpec ident="head">
					<content>
						<textNode/>
					</content>
					<attList>
						<attDef ident="xml:lang" usage="req" mode="change">
							<datatype>
								<dataRef key="teidata.language"/>
							</datatype>
						</attDef>
					</attList>
				</elementSpec>

				<elementSpec ident="desc" corresp="definition">
					<content>
						<alternate minOccurs="0" maxOccurs="unbounded">
							<textNode/>
							<elementRef key="list" minOccurs="0"/>
							<elementRef key="ref" minOccurs="0"/>
							<elementRef key="abbr" minOccurs="0"/>
							<elementRef key="expan" minOccurs="0"/>
						</alternate>
					</content>
					<attList>
						<attDef ident="type" usage="req" mode="change">
							<datatype>
								<dataRef key="teidata.enumerated"/>
							</datatype>
							<defaultVal>definition</defaultVal>
						</attDef>
						<!-- @xml:lang is required -->
						<attDef ident="xml:lang" usage="req" mode="change">
							<datatype>
								<dataRef key="teidata.language"/>
							</datatype>
						</attDef>
					</attList>
				</elementSpec>

				<elementSpec ident="desc" corresp="terms">
					<content>
						<elementRef key="term" minOccurs="0" maxOccurs="unbounded"/>
					</content>
				</elementSpec>

				<elementSpec ident="figure">
					<content>
						<sequence minOccurs="1" maxOccurs="1">
							<elementRef key="head"/>
							<elementRef key="graphic"/>
							<elementRef key="figDesc"/>
						</sequence>
					</content>
				</elementSpec>
				
				<elementSpec ident="graphic"></elementSpec>
				
				<elementSpec ident="figDesc"></elementSpec>
				
				<elementSpec ident="listEvent">
					<content>
						<elementRef key="event" minOccurs="0" maxOccurs="unbounded"/>
					</content>
				</elementSpec>
				
				<elementSpec ident="event">
					<content>
						<sequence>
							<elementRef key="head" minOccurs="1" maxOccurs="unbounded"/>
							<elementRef key="desc" corresp="definition" minOccurs="1" maxOccurs="unbounded"/>
							<elementRef key="desc" corresp="terms" minOccurs="1" maxOccurs="unbounded"/>
							<elementRef key="linkGrp" corresp="generalResources" minOccurs="0" maxOccurs="1"/>
							<elementRef key="linkGrp" corresp="projectResources" minOccurs="0" maxOccurs="1"/>
							<elementRef key="param" minOccurs="0" maxOccurs="unbounded"/>
						</sequence>
					</content>
					<attList>
						<attDef ident="ref"></attDef>
						<attDef ident="type"></attDef>
						<attDef ident="xml:id"></attDef>
					</attList>
				</elementSpec>
				
				<elementSpec ident="term">
					<content>
						<textNode/>
					</content>
					<attList>
						<attDef ident="key"></attDef>
						<attDef ident="source"></attDef>
						<attDef ident="type"></attDef>
					</attList>
				</elementSpec>
				
				<elementSpec ident="linkGrp" corresp="generalResources">
					<content>
						<elementRef key="ref" minOccurs="0" maxOccurs="unbounded"/>
					</content>
					<attList>
						<attDef ident="source"></attDef>
						<attDef ident="target"></attDef>
						<attDef ident="type"></attDef>
					</attList>
				</elementSpec>

				<elementSpec ident="linkGrp" corresp="projectResources">
					<content>
						<elementRef key="ref" minOccurs="0" maxOccurs="unbounded"/>
					</content>
					<attList>
						<attDef ident="source"></attDef>
						<attDef ident="target"></attDef>
						<attDef ident="type"></attDef>
					</attList>
				</elementSpec>
				
				<elementSpec ident="param"></elementSpec>

				<elementSpec ident="fileDesc" module="header">
					<desc xml:lang="fr">Titre, citation, diffusion et données primaires ~ signal
						audio/vidéo</desc>
					<desc xml:lang="en">Title, citation, diffusion and primary data ~ signal
						audio/vidéo</desc>
					<content>
						<sequence minOccurs="1" maxOccurs="1">
							<elementRef key="titleStmt"/>
							<elementRef key="publicationStmt"/>
							<elementRef key="sourceDesc"/>
						</sequence>
					</content>
				</elementSpec>

				<!-- titleStmt : un principal, de 0 à n respStmt (collecteur, transcripteur, collecteur, ...) -->
				<elementSpec ident="titleStmt" module="header" mode="change" n="1">
					<desc xml:lang="fr">La ressource: titre, description, citation, responsable,
						contributeurs</desc>
					<desc xml:lang="en">Resource : Title, description, citation, responsible,
						contributors</desc>
					<content>
						<elementRef key="title" minOccurs="1" maxOccurs="1"/>
						<elementRef key="author" minOccurs="1" maxOccurs="unbounded"/>
						<elementRef key="principal" minOccurs="1" maxOccurs="unbounded"/>
						<elementRef key="respStmt" minOccurs="0" maxOccurs="unbounded"/>
					</content>
				</elementSpec>

				<elementSpec ident="author" module="core">
					<content>
						<elementRef key="persName"/>
						<elementRef key="affiliation"/>
					</content>
				</elementSpec>

				<elementSpec ident="affiliation"></elementSpec>
				
				<elementSpec ident="title" module="core" mode="change">
					<desc xml:lang="fr"> Titre, description, citation</desc>
					<desc xml:lang="en"> Title, description, citation</desc>
					<content>
						<elementRef key="idno" corresp="title" minOccurs="1" maxOccurs="1"/>
						<elementRef key="bibl" minOccurs="1" maxOccurs="unbounded"/>
					</content>
				</elementSpec>


				<elementSpec ident="idno" corresp="title" module="header">
					<desc xml:lang="fr">Titre de la ressource, nom d'usage </desc>
					<desc xml:lang="fr">Resource title, usual title</desc>
					<content>
						<textNode/>
					</content>
					<attList>
						<attDef ident="type" usage="req" mode="change" rend="title">
							<valList>
								<valItem ident="title"/>
							</valList>
						</attDef>
					</attList>
				</elementSpec>

				<elementSpec ident="bibl" module="core">
					<desc xml:lang="fr">Citation(s) accompagnant la ressource: projet, équipe de
						recherche, référence bibliographique</desc>
					<desc xml:lang="fr">Citation(s) : project, research team, scientific
						reference</desc>
					<content>
						<textNode/>
					</content>
				</elementSpec>

				<elementSpec ident="principal" module="header">
					<desc xml:lang="fr">Responsable de la ressource: organisme, laboratoire, projet,
						personne</desc>
					<desc xml:lang="en">Responsible(s) of the ressource: organism, research lab,
						project, person</desc>
					<content>
						<elementRef key="name" minOccurs="1" maxOccurs="1"/>
						<!-- default : alphanum -->
						<dataRef name="NCName"/>
					</content>
				</elementSpec>

				<elementSpec ident="name" module="core">
					<desc xml:lang="fr">Nom</desc>
					<desc xml:lang="en">Name</desc>
					<content>
						<!-- default="alphanum" -->
						<dataRef name="NCName"/>
					</content>
					<attList>
						<attDef ident="type" usage="req" mode="change" rend="name.project">
							<valList mode="replace">
								<valItem ident="name.organism">
									<desc rendition="texte aide laboratoire" xml:lang="fr"
										>laboratoire ou institution</desc>
									<desc rendition="help text on search lab" xml:lang="en">Research
										laboratory or institution</desc>
								</valItem>
								<valItem ident="name.project">
									<desc rendition="texte aide projet" xml:lang="fr">projet</desc>
									<desc rendition="help texte on project" xml:lang="en"
										>project</desc>
								</valItem>
								<valItem ident="name.person">
									<desc rendition="texte aide personne" xml:lang="fr"
										>personne</desc>
									<desc rendition="help text on person" xml:lang="en"
										>person</desc>
								</valItem>
								<valItem ident="name.other">
									<desc xml:lang="fr">autre entité</desc>
									<desc xml:lang="en">other entity</desc>
								</valItem>
							</valList>
						</attDef>
					</attList>
				</elementSpec>

				<!-- respStmt dans titleStmt -->
				<elementSpec ident="respStmt" module="core">
					<desc xml:lang="fr">Contributeurs </desc>
					<desc xml:lang="en">Contributors</desc>
					<content>
						<sequence minOccurs="1" maxOccurs="unbounded">
							<elementRef key="resp"/>
							<elementRef key="name"/>
						</sequence>
					</content>
				</elementSpec>

				<elementSpec ident="resp" module="core">
					<desc xml:lang="fr">Rôle, fonction</desc>
					<desc xml:lang="en">Function</desc>
					<content>
						<valList mode="replace">
							<valItem ident="resp.annotator">
								<desc rendition="texte aide annotateur" xml:lang="fr"
									>annotateur</desc>
								<desc rendition="help text on annotator" xml:lang="en"
									>annotator</desc>
							</valItem>
							<valItem ident="resp.compiler">
								<desc rendition="texte aide coordinateur" xml:lang="fr"
									>coordinateur</desc>
								<desc rendition="help text on annotator" xml:lang="en"
									>coordinator</desc>
							</valItem>
							<valItem ident="resp.interviewer">
								<desc rendition="texte aide interviewer" xml:lang="fr"
									>interviewer</desc>
								<desc rendition="help text on interviewer" xml:lang="en"
									>interviewer</desc>
							</valItem>
							<valItem ident="resp.recorder">
								<desc rendition="texte aide collecteur" xml:lang="fr"
									>collecteur</desc>
								<desc rendition="help text on recorder" xml:lang="en"
									>recorder</desc>
							</valItem>
							<valItem ident="resp.transcriber">
								<desc rendition="texte aide transcripteur" xml:lang="fr"
									>transcripteur</desc>
								<desc rendition="help text on transcriber" xml:lang="en"
									>transcriber</desc>
							</valItem>
							<valItem ident="resp.translator">
								<desc rendition="texte aide traducteur" xml:lang="fr"
									>traducteur</desc>
								<desc rendition="help text on translator" xml:lang="en"
									>translator</desc>
							</valItem>
							<valItem ident="resp.data_inputter">
								<desc rendition="texte aide data_inputter" xml:lang="fr"
									>vérificateur - normalisateur</desc>
								<desc rendition="help text on data_inputter" xml:lang="en"
									>data_inputter</desc>
							</valItem>
							<valItem ident="resp.OTHER">
								<desc xml:lang="fr">autre rôle</desc>
								<desc xml:lang="en">other role</desc>
							</valItem>
						</valList>
					</content>
				</elementSpec>

				<!-- publicationStmt : au moins un , un handle et une licence
	  				par defaut Creative Common CC BY-NC-SA 2.0 FR c'est à dire
	  				"Attribution - Pas d’Utilisation Commerciale - Partage dans les Mêmes Conditions"-->
				<elementSpec ident="publicationStmt" module="header" mode="change" n="1">
					<desc xml:lang="fr">Diffusion : identifiant unique, diffusion, sites web,
						licence</desc>
					<desc xml:lang="en">Diffusion : handle, publisher, pubPlace, license</desc>
					<content>
						<elementRef key="authority"/>
						<elementRef key="publisher" minOccurs="1" maxOccurs="unbounded"/>
						<elementRef key="pubPlace" minOccurs="0" maxOccurs="unbounded"/>
						<!-- carole à vérifier avec Loïc : zero car je ne suis pas sure que chaque ressource aura un handle si elle n'est pas ds ortolang ou cocoon,
	  					more:la même ressource peut avoir plusieurs handles si déposée sur plusieurs plateformes
		  				pas de type car handle alphanum + certains caractères spéciaux ? -->
						<elementRef key="idno" corresp="handle" minOccurs="0" maxOccurs="unbounded"/>
						<elementRef key="availability" minOccurs="1" maxOccurs="unbounded"/>
					</content>
				</elementSpec>

				<elementSpec ident="availability" module="header">
					<desc xml:lang="fr">Conditions de diffusion</desc>
					<desc xml:lang="fr">Conditions de diffusion</desc>
					<content>
						<elementRef key="licence" minOccurs="1" maxOccurs="unbounded"/>
					</content>
					<attList>
						<!-- value=restricted car une creative common telle qu'elle soit-->
						<attDef ident="status" usage="req" mode="change" rend="restricted">
							<valList>
								<valItem ident="restricted"/>
							</valList>
						</attDef>
					</attList>
				</elementSpec>

				<elementSpec ident="licence" module="header">
					<desc xml:lang="fr">License de diffusion</desc>
					<desc xml:lang="fr">Diffusion Licence</desc>
					<content>
						<elementRef key="p" minOccurs="1" maxOccurs="1"/>
					</content>
					<attList>
						<attDef ident="type" usage="req" mode="change" rend="licence.type.BY_NC_SA">
							<valList mode="replace">
								<valItem ident="licence.type.BY_NC_SA">
									<desc xml:lang="fr">Creative Common CC_BY_NC_SA : Attribution
										sans usage commercial et partage suivant les mêmes modalités </desc>
									<desc xml:lang="en">Creative Common CC_BY_NC_SA : Attribution
										and no Commercial use and share in the same way</desc>
								</valItem>
								<valItem ident="licence.type.BY">
									<desc xml:lang="fr">Creative Common CC_BY : Attribution</desc>
									<desc xml:lang="en">Creative Common CC_BY : Attribution</desc>
								</valItem>
								<valItem ident="licence.type.BY_ND">
									<desc xml:lang="fr">Creative Common CC_BY_ND : Attribution sans
										modification</desc>
									<desc xml:lang="en">Creative Common CC_BY_ND : Attribution and
										no modification</desc>
								</valItem>
								<valItem ident="licence.type.BY_NC_ND">
									<desc xml:lang="fr">Creative Common CC_BY_NC_NDY : Attribution
										sans usage commercial et sans modification</desc>
									<desc xml:lang="en">Creative Common CC_BY_NC_ND : Attribution
										and no commercial use and no modification</desc>
								</valItem>
								<valItem ident="licence.type.BY_NC">
									<desc xml:lang="fr">Creative Common CC_BY_NC : Attribution sans
										usage commercial</desc>
									<desc xml:lang="en">Creative Common CC_BY_NC : Attribution and
										no commercial use</desc>
								</valItem>

								<valItem ident="licence.type.BY_SA">
									<desc xml:lang="fr">Creative Common CC_BY_SA : Attribution et
										partage suivant les mêmes modalités </desc>
									<desc xml:lang="en">Creative Common CC_BY_SA : Attribution and
										share in the same way</desc>
								</valItem>
								<valItem ident="licence.type.OTHER">
									<desc xml:lang="fr">autre license</desc>
									<desc xml:lang="en">other licence</desc>
								</valItem>
							</valList>
						</attDef>
						<attDef ident="target">
							<datatype>
								<dataRef key="anyURI"/>
							</datatype>
						</attDef>
					</attList>
				</elementSpec>

				<elementSpec ident="authority">
					<content>
						<textNode/>
					</content>
				</elementSpec>

				<elementSpec ident="publisher" module="core">
					<desc xml:lang="fr">Projet, archive diffusant la ressource</desc>
					<desc xml:lang="en">Project,archive for ressource distribution</desc>
					<content>
						<textNode/>
						<dataRef key="anyURI"/>
					</content>
				</elementSpec>

				<elementSpec ident="pubPlace" module="core">
					<desc xml:lang="fr">Diffusion dans d'autres sites</desc>
					<desc xml:lang="en">Other diffusion places</desc>
					<content>
						<elementRef key="ref" minOccurs="1" maxOccurs="unbounded"/>
						<textNode/>
					</content>
				</elementSpec>

				<elementSpec ident="idno" corresp="handle" module="header">
					<desc xml:lang="fr">Identifiant unique de la ressource</desc>
					<desc xml:lang="en">Handle</desc>

					<content>
						<textNode/>
					</content>
					<attList>
						<attDef ident="type" usage="req" mode="change" rend="idno.handle">
							<valList>
								<valItem ident="idno.handle">
									<desc xml:lang="fr">handle</desc>
									<desc xml:lang="en">handle</desc>
								</valItem>
								<valItem ident="idno.ISSN">
									<desc xml:lang="fr">ISSN</desc>
									<desc xml:lang="en">ISSN</desc>
								</valItem>
								<valItem ident="idno.ISBN">
									<desc xml:lang="fr">ISBN</desc>
									<desc xml:lang="en">ISBN</desc>
								</valItem>
								<valItem ident="idno.URI">
									<desc xml:lang="fr">URI</desc>
									<desc xml:lang="en">URI</desc>
								</valItem>
								<valItem ident="idno.DOI">
									<desc xml:lang="fr">DOI</desc>
									<desc xml:lang="en">DOI</desc>
								</valItem>
								<valItem ident="idno.ESTC">
									<desc xml:lang="fr">ESTC</desc>
									<desc xml:lang="en">ESTC</desc>
								</valItem>
								<valItem ident="idno.OCLC">
									<desc xml:lang="fr">OCLC</desc>
									<desc xml:lang="en">OCLC</desc>
								</valItem>
								<valItem ident="idno.library">
									<desc xml:lang="fr">library</desc>
									<desc xml:lang="en">library</desc>
								</valItem>
								<valItem ident="idno.OTHER">
									<desc xml:lang="fr">autre identifiant</desc>
									<desc xml:lang="en">other handle</desc>
								</valItem>
							</valList>
						</attDef>
					</attList>
				</elementSpec>

				<elementSpec ident="ref">
					<desc xml:lang="fr">URL</desc>
					<desc xml:lang="fr">URL</desc>
					<content>
						<textNode/>
					</content>
					<attList>
						<attDef ident="target" usage="req" mode="change">
							<desc xml:lang="fr">Lien vers la ressource (URL)</desc>
							<desc xml:lang="en">URL</desc>
							<datatype>
								<dataRef name="anyURI"/>
							</datatype>
						</attDef>
					</attList>
				</elementSpec>

				<!-- recordingStmt : recording et medias-->
				<elementSpec ident="sourceDesc" module="header" mode="change" n="1">
					<desc xml:lang="fr">Données primaires (signal)</desc>
					<desc xml:lang="en">Primary data (recording)</desc>
					<content>
						<elementRef key="p" minOccurs="1" maxOccurs="1"/>
					</content>
				</elementSpec>

				<elementSpec ident="persName" module="namesdates">
					<desc xml:lang="fr">Nom du locuteur</desc>
					<desc xml:lang="en">Speaker's name</desc>
					<content>
						<textNode/>
					</content>
				</elementSpec>

				<elementSpec ident="placeName" module="namesdates">
					<desc xml:lang="fr">Lieu</desc>
					<desc xml:lang="en">Place</desc>
					<content>
						<textNode/>
					</content>
				</elementSpec>

				<elementSpec ident="country" module="namesdates">
					<desc xml:lang="fr">Pays</desc>
					<desc xml:lang="en">Country</desc>
					<attList>
						<!-- attribut ref= norme iso ISO 3366-1 => Indiquer le pays de recueil , donnée minimale en général connue, fr par défaut -->
						<attDef ident="name" usage="req" mode="change" rend="fr"/>
					</attList>
				</elementSpec>

				<elementSpec ident="district" module="namesdates">
					<desc xml:lang="fr">Région</desc>
					<desc xml:lang="en">District</desc>
					<attList>
						<attDef ident="name" usage="rec" mode="change"/>
					</attList>
				</elementSpec>

				<elementSpec ident="settlement" module="namesdates">
					<desc xml:lang="fr">Ville</desc>
					<desc xml:lang="en">Town</desc>
					<attList>
