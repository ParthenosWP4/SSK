                     <oneOrMore>
                        <ref name="title"/>
                     </oneOrMore>
            
            
                     <zeroOrMore>
                        <choice>
                
                           <ref name="idno"/>
                           <ref name="textLang"/>
                           <ref name="editor"/>
                           <ref name="respStmt"/>
                        </choice>
                     </zeroOrMore>
            
                  </group>
                  <group>
            
                     <oneOrMore>
                        <choice>
                           <ref name="title"/>
                
                           <ref name="idno"/>
                        </choice>
                     </oneOrMore>
            
            
                     <zeroOrMore>
                        <choice>
                           <ref name="textLang"/>
                           <ref name="author"/>
                           <ref name="editor"/>
                           <ref name="meeting"/>
                           <ref name="respStmt"/>
                        </choice>
                     </zeroOrMore>
            
                  </group>
                  <group>
            
                     <ref name="authority"/>
                     <ref name="idno"/>
                  </group>
               </choice>
            </optional>
      
      
            <zeroOrMore>
               <ref name="availability"/>
            </zeroOrMore>
      
      
        
      
            <zeroOrMore>
               <group>
                  <ref name="edition"/>
        
